{
    "name": "bijanmmarkes/gtfs-realtime-php-sdk",
    "description": "PHP Classes and custom buffer for integrating GTFS real-time into PHP projects.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Google\\Protobuf\\": "src/Google/Protobuf",
            "GPBMetadata\\Google\\Protobuf\\": "src/GPBMetadata/Google/Protobuf",
            "Google\\Transit\\Realtime\\": "src/gtfs/Google/Transit/Realtime",
            "GPBMetadata\\Protobuf\\": "src/gtfs/GPBMetadata/Protobuf"
        }
    },
    "authors": [
        {
            "name": "bijanmmarkes",
            "email": "bijammarkes@gmail.com"
        }
    ],
    "require-dev": {
        "protobuf-php/protobuf": "^0.1.3",
        "protobuf-php/protobuf-plugin": "^0.1.3"
    }
}
