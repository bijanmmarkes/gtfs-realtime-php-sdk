{
    "name": "bijanmmarkes/gtfs-realtime-php-sdk",
    "version": "1.0.0",
    "description": "PHP Library to parse and create gtfs real-time feeds using the php protobuf wrapper. Parse and Create binary Protobuf files for GTFS Realtime feeds.",
    "type": "library",
    "keywords": ["php", "gtfs", "gtfs-rt", "gtfs realtime", "library", "protobuf", "trip updates", "service alerts", "vehicle positions", "gtfs sdk", "gtfs-rt php sdk"],
    "autoload": {
        "psr-4": {
            "Google\\Protobuf\\": "src/Google/Protobuf",
            "GPBMetadata\\Google\\Protobuf\\": "src/GPBMetadata/Google/Protobuf",
            "Google\\Transit\\Realtime\\": "src/GtfsRealtime/Google/Transit/Realtime",
            "GPBMetadata\\Protobuf\\": "src/GtfsRealtime/GPBMetadata/Protobuf"
        }
    },
    "authors": [
        {
            "name": "Bijan Markes",
            "email": "bijammarkes@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 | ^8.0 | ^8.1 | ^8.2"
    },
    "require-dev": {
        "protobuf-php/protobuf": "^0.1.3",
        "protobuf-php/protobuf-plugin": "^0.1.3"
    }
}
